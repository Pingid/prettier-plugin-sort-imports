{
  "name": "@pingid/prettier-plugin-sort-imports",
  "version": "0.0.6",
  "description": "A tiny Prettier plugin that keeps your import statements tidy.",
  "author": "Dan Beaven <dm.beaven@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pingid/prettier-plugin-sort-imports.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsc && unbuild",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "prettier": "^3.5.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "all"
  }
}
